<h1>Usuarios:</h1>

<table class="table">  
    <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Email</th>
            <th scope="col">Age</th>
            <th scope="col">Role</th>
            <th scope="col">Last Connection</th>
            <th scope="col"></th>
        </tr>
   </thead> 
   <tbody>
        {{#each users}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.first_name}}</td>
                <td>{{this.last_name}}</td>
                <td>{{this.email}}</td>
                <td>{{this.age}}</td>
                <td>{{this.role}}</td>
                <td>{{this.last_connection}}</td>
                <td><input class="btn btn-warning" type="button" value="Editar"><div class="btnDeleteUser"></div></td>
            </tr>
        {{/each}}
    
    </tbody>




</table>
